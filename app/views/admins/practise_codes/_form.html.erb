<div class="panel-body">
  <%= simple_form_for(@admins_practise_code, url: @admins_practise_code.new_record? ? admins_practise_codes_path : admins_practise_code_path(@admins_practise_code), html: { class: "col-md-12" }) do |f| %>
    <%= f.input :code, input_html: { class: "form-control", autofocus: true } %><br/>
    <%= f.submit "Publish PractiseCode", class: "btn btn-primary" %>
    <%= link_to "Cancel", admins_practise_codes_path, class: "btn btn-defaut" %>
  <% end %>
</div>
