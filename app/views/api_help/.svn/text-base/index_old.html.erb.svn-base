        <script type="text/javascript">
            var currentTarget = "";
            var currentRaw	  = "";
            function example(name)
            {
                currentTarget = $("#output_" + name);
                currentRaw = $("#raw_" + name);
                currentTarget.html("..loading..");
                var link = $("#example_" + name).val();
                var method = $("#method_" + name).text();

                // remove accessing api_raw url during POST method
                if (method!="POST"){
                    currentRaw.html("..loading..");
                    $.ajax({
                        type:       method,
                        url:        "api_raw.php",
                        data:       { raw : link + "&api_log=false" }
                    }).done(function(output){
                        currentRaw.html(output);
                    });
                }

                // we dont want to destroy the existing program, that's why we copy the original and make one
                if (method=="POST"){
                    var link = $("#link_" + name).html();
                    $.ajax({
                        type:       method,
                        url:        link,
                        data:       $('.form_'+name).serialize(),
                        processData: true,
                        error: function(xhr, ajaxOptions, thrownError) {
                            currentTarget.text("Error " + xhr.status + " " + xhr.responseText);
                        }
                    }).done(function(output){
                        console.log("Output = ", output);
                        if (typeof(output) == "object")
                        {
                            var txt = "Received JSON Object:\n<br/>";
                            txt = prettyPrint( output );
                            currentTarget.html(txt);
                        } else {
                            currentTarget.text("Raw output: " + output);
                        }
                    })

                }else{
                    $.ajax({
                        type:   method,
                        url:    link,
                        processData: true,
                        error: function(xhr, ajaxOptions, thrownError) {
                            currentTarget.text("Error " + xhr.status + " " + xhr.responseText);
                        }
                    }).done(function(output){
                        console.log("Output = ", output);
                        if (typeof(output) == "object")
                        {
                            var txt = "Received JSON Object:\n<br/>";
                            txt = prettyPrint( output );
                            currentTarget.html(txt);
                        } else {
                            currentTarget.text("Raw output: " + output);
                        }
                    })
                }
            }

            function exampleWithFile(name)
            {
                currentTarget = $("#output_" + name);
                currentRaw = $("#raw_" + name);
                currentRaw.html("..loading..");
                currentTarget.html("..loading..");
                var link = $("#example_" + name).val();
                var method = $("#method_" + name).text();
                $.ajax({
                    type:       method,
                    url:        "api_raw.php",
                    data:		{ raw : link }
                }).done(function(output){
                    currentRaw.html(output);
                });

                $.ajax({
                    type:   method,
                    url:    link,
                    processData: false
                }).done(function(output){
                    console.log("Output = ", output);
                    if (typeof(output) == "object")
                    {
                        var txt = "Received JSON Object:\n<br/>";
                        txt = prettyPrint( output );
                        currentTarget.html(txt);
                    } else {
                        currentTarget.text("Raw output: " + output);
                    }
                }).fail(function(obj, output)
                {
                    currentTarget.text("FAILED: " + output);
                })
            }

            function change(name)
            {
                console.log("CHANGE", name);
            }
        </script>

    </head>
