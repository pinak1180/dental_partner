<!-- Login Api -->
  <%= render partial: 'api', locals: {
        link_name: 'Login',
        link_href: 'login',
        link_method: 'POST',
        original_link: api_dental_partner_login_path,
        parameters: [
          {name: 'email', description: 'Email Address of User', required: 1 },
          {name: 'password', description: 'Password', required: 1},
          {name: 'device_id', description: 'Device Id', required: 1},
          {name: 'device_type', description: 'Device type', required: 1}

        ],
        options: {}} %>

<!-- Change Password Api -->
  <%= render partial: 'api', locals: {
        link_name: 'Change Password',
        link_href: 'change_password',
        link_method: 'POST',
        original_link: api_dental_partner_change_password_path,
        parameters: [
          {name: 'authentication_token', description: 'You will get authentication token from signup/login api', required: 1 },
          {name: 'user[current_password]', description: 'Current Password', required: 1 },
          {name: 'user[password]', description: 'New Password', required: 1 },
          {name: 'user[password_confirmation]', description: 'New Password', required: 1 }


        ],
        options: {}} %>

 <!-- Forgot Password Api -->
  <%= render partial: 'api', locals: {
        link_name: 'Forgot Password Api',
        link_href: 'forgot_password',
        link_method: 'POST',
        original_link: api_dental_partner_forgot_password_path,
        parameters: [
          {name: 'email', description: 'Email Address of User', required: 1 }
        ],
        options: {}} %>

<!-- Logout -->
  <%= render partial: 'api', locals: {
        link_name: 'Logout',
        link_href: 'logout',
        link_method: 'GET',
        original_link: api_dental_partner_logout_path,
        parameters: [
          {name: 'authentication_token', description: 'You will get authentication token from signup/login api', required: 1 }

        ],
        options: {}} %>
